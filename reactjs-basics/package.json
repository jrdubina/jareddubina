{
  "name": "reactjs-basics",
  "version": "1.0.0",
  "description": "Some Basic Reactjs",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d & copy \"src/index.html\" \"dist/index.html\" /Y & webpack-dev-server --content-base src/ --inline --watch",
    "build:prod": "webpack -p & copy \"src/index.html\" \"dist/index.html\" /Y"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Jared Dubina",
  "license": "MIT",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
}
